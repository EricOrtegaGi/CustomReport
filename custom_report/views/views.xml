<odoo>
    <data>
        <record id="view_custom_report_tree" model="ir.ui.view">
            <field name="name">custom.report.tree</field>
            <field name="model">custom.report</field>
            <field name="arch" type="xml">
                <tree string="Module Report">
                    <field name="total_price"/>
                    <field name="comercial"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <record id="action_custom_report" model="ir.actions.act_window">
            <field name="name">Module Report</field>
            <field name="res_model">custom.report</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_custom_report_tree"/>
        </record>

        <menuitem id="menu_custom_report" name="Module Report" parent="base.menu_reporting" sequence="20" action="action_custom_report"/>
    </data>
</odoo>